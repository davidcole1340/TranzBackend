{
  "name": "tranzql",
  "version": "0.0.1",
  "description": "GraphQL server for Tranzit and AT information",
  "main": "index.js",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./index.ts",
    "start:watch": "nodemon"
  },
  "author": "David Cole <david.cole1340@gmail.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "bent": "^7.3.10",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.2.3",
    "lodash.merge": "^4.6.2",
    "mongodb": "^3.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/lodash.merge": "^4.6.6",
    "@types/mongodb": "^3.5.27",
    "@types/node": "^14.11.2",
    "@types/bent": "^7.3.0",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": "./",
    "exec": "npm start",
    "ext": "ts,graphql"
  }
}
